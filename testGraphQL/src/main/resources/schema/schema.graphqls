type Query{
  authors: [Author!]!
  books : [Book!]!
}

type Mutation {
  createAuthor(name: String!): Author!
  updateAuthor(id:ID, name: String!): Author!
  deleteAuthor(id: ID!): ID!
  createBook(title:String!, publication_year: Int!, authorId: ID!): Book!
}

type Author {
  id: ID
  name: String!
  books: [Book!]
}

type Book {
  id: ID
  title: String!
  publication_year: Int!
  author: Author
}